{
  "name": "koa-skeleton",
  "version": "0.1.0",
  "author": "weberliu",
  "scripts": {
    "start": "export NODE_ENV=development; node bin/development.js",
    "production": "node bin/production.js",
    "debug": "npm run clean && node bin/debug",
    "build": "npm test && npm run clean && npm run compile",
    "compile": "babel src/ --out-dir app/ --retain-lines --source-maps",
    "clean": "rm -rf app/",
    "test": "export NODE_ENV=test; export DEBUG=; mocha -u bdd --compilers js:babel-core/register",
    "dev": "npm start",
    "migrate": "node bin/migrate.js"
  },
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "bcrypt": "^1.0.2",
    "co": "^4.6.0",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "ejs": "^2.4.1",
    "fs": "0.0.1-security",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.0.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-onerror": "^3.1.0",
    "koa-restql": "^1.0.8",
    "koa-router": "^7.0.0",
    "koa-sequelize-resource": "^0.1.3",
    "koa-static-plus": "^0.1.1",
    "koa-views": "^5.0.1",
    "lodash": "^4.11.1",
    "mysql": "^2.13.0",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "child_process": "^1.0.2",
    "chokidar": "^1.5.1",
    "colors": "^1.1.2",
    "eslint": "^2.11.1",
    "eslint-config-standard": "^5.2.0",
    "eslint-plugin-promise": "^1.3.1",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^3.5.0",
    "should": "^8.3.0",
    "supertest": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weberliu/koa-skeleton.git"
  },
  "license": "MIT"
}
